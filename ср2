#include <iostream>

int main() {
    bool result;
    int num;
    std::cout << std::boolalpha;
    std::cout << "Введите число: ";
    std::cin >> num;
    if (num % 10 + num / 10 % 10 == num / 100 % 10 + num / 1000) result = true;
    else result = false;
    std::cout << result << std::endl;
    return 0;
}
